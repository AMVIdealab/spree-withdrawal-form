<%=form_for @withdrawal_form do |f|%>
  
  <p>
    <%= f.label :service_name%> <br>
    <%= f.text_field :service_name%> 
  </p>
  
  <p>
    <%= f.label :order_number%><br>
    <%= f.text_field :order_number%>
  </p>
  
  <p>
    <%= f.label :order_date%>
    <%= f.date_field :order_date%>
  </p>

  <p>
    <%= f.label :name%><br>
    <%= f.text_field :name%>
  </p>
  <p>
    <%= f.label :last_name%><br>
    <%= f.text_field :last_name%>
  </p>

  <p>
    <%= f.label :email%><br>
    <%= f.text_field :email%>
  </p>
  
  <p>
    <%= f.label :address%><br>
    <%= f.text_field :address%>
  </p>
  
  <%= f.submit%>

<%end%>
